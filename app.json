{
  "expo": {
    "name": "Hinnap√µx",
    "slug": "hinnapox_app",
    "version": "1.9.98",
    "scheme": "hinnapox",
    "web": {
      "bundler": "metro",
      "output": "static",
      "appPath": "./",
      "favicon": "./assets/icon.png"
    },
    "experiments": {
      "baseUrl": "/hinnapox_app",
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.hinnapox-app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false 
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.hinnapox_app"
    },
    "extra": {
      "eas": {
        "projectId": "aace0dca-c152-45d1-9d12-503386eb14f6"
      }
    },
    "plugins": ["expo-router"]
  }
}
